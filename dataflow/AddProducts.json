{
	"name": "AddProducts",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "productssource",
						"type": "DatasetReference"
					},
					"name": "products"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "dimProductTarget",
						"type": "DatasetReference"
					},
					"name": "ActiveRecords"
				}
			],
			"transformations": [
				{
					"name": "AddNullEndDate"
				}
			],
			"scriptLines": [
				"source(output(",
				"          ProductId as integer,",
				"          ProductName as string,",
				"          ListPrice as decimal(19,4),",
				"          ProductModelID as integer,",
				"          ModelName as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'select ProductId,bp.Name ProductName,ListPrice,pm.ProductModelID,pm.Name as ModelName\\nfrom dbo.bigProduct bp\\ninner join SalesLT.ProductModel pm\\non bp.ProductModelID=pm.ProductModelId',",
				"     format: 'query') ~> products",
				"products derive(EndDate = toDate(null())) ~> AddNullEndDate",
				"AddNullEndDate sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          ProductKey as integer,",
				"          ProductID as integer,",
				"          Name as string,",
				"          ListPrice as decimal(19,4),",
				"          ProductModelID as integer,",
				"          ModelName as string,",
				"          EndDate as timestamp",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          ProductID = ProductId,",
				"          Name = ProductName,",
				"          ListPrice,",
				"          ProductModelID,",
				"          ModelName,",
				"          EndDate",
				"     )) ~> ActiveRecords"
			]
		}
	}
}